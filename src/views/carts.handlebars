<h1>Carrito # {{cart}}</h1>

  <table>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Titulo</th>
      <th scope="col">Descripcion</th>
      <th scope="col">Precio</th>
      <th scope="col">Link imagen</th>
      <th scope="col">Codigo</th>
      <th scope="col">Stock</th>
      <th scope="col">Categoria</th>
      <th scope="col">Estado</th>
      <th scope="col">Quantity</th>
    </tr>
    <tbody>
      {{#each result}}
      <tr>
        <td>{{this.product._id}}</td>
        <td>{{this.product.title}}</td>
        <td>{{this.product.description}}</td>
        <td>{{this.product.price}}</td>
        <td>{{this.product.thumbnail}}</td>
        <td>{{this.product.code}}</td>
        <td>{{this.product.stock}}</td>
        <td>{{this.product.category}}</td>
        <td>{{#if this.product.status}}Habilitado {{else}}Deshabilitado{{/if}}</td>
        <td>{{this.quantity}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>